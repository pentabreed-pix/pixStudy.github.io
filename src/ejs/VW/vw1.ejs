    -moz-transform: none;
    -ms-transform: none;
    -o-transform: none;
    transform: none
}

.wwc-section-bg-front .wwc-back {
    background: 50% 50% no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0
}

.wwc-section-bg-front.active .wwc-bg {
    opacity: 1;
    -webkit-transform: translateY(0);
    -moz-transform: translateY(0);
    -ms-transform: translateY(0);
    -o-transform: translateY(0);
    transform: translateY(0)
}

.wwc-section-bg-front.active .wwc-bg.wwc-section-bg-front1 {
