<script>

$(function(){
    var $scrollArea = $('#wwcMain');  // 스크롤 감지 대상
    var $wwcHistoryBlocks = $('.wwc-history-block');
    var lastActiveIndex = -1;
    var isAutoScrolling = false; // ✅ 자동 스크롤 방지 변수

    function checkHistory() {
        if (isAutoScrolling) return;  // ✅ 자동 스크롤 중이면 이벤트 실행 안 함

        console.log("📌 checkHistory() 실행됨!");

        var scrollTop = $scrollArea.scrollTop();
        var windowHeight = $(window).height();
        var triggerPoint = windowHeight * 0.7; // ✅ 블록이 70% 이상 덮였을 때만 변경

        var closestIndex = -1;
        var minDistance = Infinity;

        $wwcHistoryBlocks.each(function(index) {
            var blockTop = $(this).offset().top - scrollTop;
            var blockHeight = $(this).outerHeight();
            var blockBottom = blockTop + blockHeight;

            // ✅ 블록이 70% 이상 덮였을 때만 다음 블록으로 변경
            if (blockBottom < triggerPoint && blockBottom > -windowHeight * 0.3) {
                if (Math.abs(blockBottom - triggerPoint) < minDistance) {
                    minDistance = Math.abs(blockBottom - triggerPoint);
                    closestIndex = index;
                }
            }
        });

        if (closestIndex !== lastActiveIndex && closestIndex !== -1) {
            console.log(`✅ 새로운 활성화 블록: index ${closestIndex}`);

            $wwcHistoryBlocks.removeClass('active');
            $wwcHistoryBlocks.eq(closestIndex).addClass('active');

            setHistory($wwcHistoryBlocks.eq(closestIndex));
            lastActiveIndex = closestIndex;
        }
    }

    // ✅ 자연스럽게 스크롤 이동 (부드러운 애니메이션 적용)
    function smoothScrollTo(targetPosition, duration = 800) {
        isAutoScrolling = true;
        let start = $scrollArea.scrollTop();
        let startTime = null;

        function animationStep(timestamp) {
            if (!startTime) startTime = timestamp;
            let progress = timestamp - startTime;
            let easeInOut = progress / duration;

            let newPosition = start + (targetPosition - start) * easeInOut;
            $scrollArea.scrollTop(newPosition);

            if (progress < duration) {
                requestAnimationFrame(animationStep);
            } else {
                isAutoScrolling = false;
            }
        }

        requestAnimationFrame(animationStep);
    }

    window.setHistory = function(history) {
        if (!history || history.length === 0) {
            console.error("setHistory 오류: history 요소가 존재하지 않음");
            return;
        }

        var $history = $(history);
        var index = $history.attr('data-index');

        console.log("✅ setHistory 실행됨! 현재 index:", index);

        var year = $('.wwc-year', $history).text();
        var month = $('.wwc-month', $history).text();
        var title = $('.wwc-history-title', $history).text();
        var descriptions = $('.wwc-history-descriptions p', $history).text();

        $('.wwc-year', $('.wwc-history-viewer')).text(year);
        $('.wwc-month', $('.wwc-history-viewer')).text(month);
        $('.wwc-history-title', $('.wwc-history-viewer')).text(title);
        $('.wwc-history-descriptions p', $('.wwc-history-viewer')).text(descriptions);

        // ✅ 배경 업데이트
        $('.wwc-history-bg-item').attr('data-status', 'next');
        $('.wwc-history-bg-item[data-index="' + index + '"]').attr('data-status', 'active')
            .prevAll().attr('data-status', 'prev');

        // ✅ 블록 변경 시 자동 스크롤 방지
        setTimeout(() => {
            isAutoScrolling = false;
        }, 500);
    };

    $scrollArea.on('scroll', function() {
        checkHistory();
    });

    checkHistory();
});

</script>
	


