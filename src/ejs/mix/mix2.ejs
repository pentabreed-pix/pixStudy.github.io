			//markers: true,
			//toggleActions: 'play none none reverse',
			start: () => {
					if (window.innerWidth >= 1024) {
						return (12.5 * (windowInnerHeight / 1)) + 'px top';
					} else {
						return (12.5 * (windowInnerHeight / 1)) + 'px top';
					}
			},
			end: () => {
					if (window.innerWidth >= 1024) {
						return (13.5 * (windowInnerHeight / 1)) + 'px top';
					} else {
						return (13.5 * (windowInnerHeight / 1)) + 'px top';
					}
			},
			onEnter: () => {
				//console.log('들어왔슈')
				$('.story-01').addClass('action-10');
				$('.post-08 .video-list .video-item').css('transition', 'all .5s ease-out');

				mySwiper.enable();	
			},
			onEnterBack: () => {
				//console.log('나갔다가 들어왔유');
				$('.story-01').removeClass('action-11');
			},
			onLeave: () => {
				$('.story-01').addClass('action-10');
				$('.story-01').addClass('action-11');
			},
			onLeaveBack: () => {
				//console.log('나갔다가 들어왔유22');
				$('.story-01').removeClass('action-10');

				mySwiper.disable();	
				mySwiper.setTranslate(0);
				mySwiper.update();
			},
		});
	}
</script>
